{
	"name": "Unblock Adblock",
	"description": "Adblock Blocker Blocker",
	"version": "1.0",
	"manifest_version": 3,
	"action": {},
	"icons": { 
		"16": "favicon.png",
		"48": "favicon.png",
		"128": "favicon.png"  
	},
	"permissions": [
	    "scripting",
	    "storage",
	    "tabs",
        "notifications"
	],
	"host_permissions": [
	    "http://*/",
        "https://*/",
        "<all_urls>"
	],
	"background": {
	  "service_worker": "background.js"
	}
}
